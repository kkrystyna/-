
using namespace std;

int main()
{
    cout << (xv-y)->((y^-z)->(xvy)) << \n\n;
    bool x,y,z;
    cout<< "x";
    cin>> x;
    cout<<"y";
    cin>>y;
    cout<<"z";
    cin>>z;


    bool part;
    part = !x&&y;
    cout<<"_______________________________________"<<endl;
    cout<<"|"<<" x ^ y"<<"|"<<setw(1)<<part1<<"|"<<endl;
    cout<<"_______________________________________"<<endl;

    bool part2;
    part2=|y| |z|;
    cout<<"_______________________________________"<<endl;
    cout<<"|"<<"-y v z "<<"|"<<setw(1)<<part2<<"|"<<endl;
    cout<<"_______________________________________"<<endl;

    bool part3;
    part3= !x&&y;
    cout<<"_______________________________________"<<endl;
    cout<<"|"<<"x v y "<<"|"<<setw(1)<<part3<<"|"<<endl;
    cout<<"_______________________________________"<<endl;

    bool part4;
    part4= part3||part4;
    cout<<"_______________________________________"<<endl;
    cout<<"|"<< " (y^-z)->(xvy)"<<"|"<<setw(1)<<part4<<"|"<<endl;
    cout<<"_______________________________________"<<endl;

    bool part5;
    part5=part1||part4;
    cout<<"_______________________________________"<<endl;
    cout<<"|"<<" "<<"|"<<setw(1)<<part5<<"|"<<endl;
    cout<<"_______________________________________"<<endl;



}
